{
  "name": "concompte",
  "version": "0.0.0",
  "private": true,
  "main": "index.js",
  "repository": "https://github.com/Hiswe/concompte.git",
  "author": "Hiswe <hiswehalya@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && parallelshell \"npm run serve:api\" \"npm run serve:app\"",
    "test": "ava --verbose",
    "build": "npm run build:icons && build:app",
    "build:icons": "gulp icons",
    "build:app": "webpack --config webpack.config.js",
    "watch:app": "webpack --config webpack.config.js --watch --display minimal",
    "serve:app": "node dist/server.js",
    "serve:app:dev": "nodemon dist/server.js --config nodemon.json",
    "serve:app:watch": "parallelshell \"npm run serve:app:dev\"  \"npm run watch:app\"",
    "serve:api": "node api/index.js",
    "serve:api:dev": "NODE_DEBUG=api,api:db,api:mailing node-dev api/index.js --no-deps --no-notify",
    "serve:api:nodemon": "nodemon api/index.js --config nodemon.api.json",
    "serve:all": "parallelshell \"npm run serve:app\" \"npm run serve:api\"",
    "serve:all:dev": "parallelshell \"npm run serve:api:dev\"  \"npm run serve:app:dev\" --verbose",
    "browsersync": "browser-sync start --config browser-sync-config.js",
    "dev:api": "npm run serve:api:dev",
    "dev:app": "parallelshell \"npm run serve:app:watch\" \"npm run browsersync\"",
    "dev:no-build": "parallelshell \"npm run watch:app\" \"npm run serve:all:dev\" \"npm run browsersync\"",
    "dev": "npm run build:app && npm run dev:no-build",
    "toc": "doctoc README.md --github"
  },
  "ava": {
    "files": [
      "shared/**/*.spec.{js,jsx}"
    ],
    "babel": false,
    "compileEnhancements": false,
    "require": [
      "@babel/register",
      "./test-browser-env.js"
    ]
  },
  "engines": {
    "node": "8.10.0"
  },
