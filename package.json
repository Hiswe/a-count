{
  "name": "a-count",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "repository": "https://github.com/Hiswe/a-count.git",
  "author": "Hiswe <hiswehalya@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "lerna run build && lerna run --parallel serve",
    "build": "lerna run build",
    "dev": "lerna run --parallel dev",
    "release": "lerna publish --skip-npm --force-publish=* && yarn run release:git",
    "release:git": "lerna run build --scope=web-app && parallelshell \"node bin/release-api.js\" \"node bin/release-webapp.js\"",
    "toc": "doctoc README.md DOCUMENTATION.md ISOMORPHIC-OVERVIEW.md --github"
  },
  "engines": {
    "node": "8.10.0"
  },
  "dependencies": {},
  "devDependencies": {
    "doctoc": "^1.3.1",
    "lerna": "^2.11.0",
    "parallelshell": "^3.0.2",
    "shelljs": "^0.8.1"
  }
}
