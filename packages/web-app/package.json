{
  "name": "web-app",
  "version": "1.0.3",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "8.10.0"
  },
  "scripts": {
    "browsersync": "browser-sync start --config browser-sync-config.js",
    "build": "yarn run build:icons && yarn run build:app --env.production",
    "build:icons": "gulp icons",
    "build:app": "webpack --config webpack.config.js",
    "dev": "parallelshell \"yarn run serve:watch\" \"yarn run browsersync\"",
    "dev:https": "acountApp_API_URL=https://localhost:4443/v1 parallelshell \"yarn run proxy\" \"yarn run serve:watch\" \"yarn run browsersync --https --proxy https://localhost:3443\"",
    "proxy": "node dev-proxy.js",
    "serve": "NODE_ENV=production node application-server.js",
    "serve:dev": "nodemon application-server.js --config nodemon.json",
    "serve:watch": "parallelshell \"yarn run serve:dev\"  \"yarn run watch\"",
    "start": "yarn run build && yarn run serve",
    "test": "ava --verbose",
    "watch": "webpack --config webpack.config.js --watch --display minimal"
  },
  "author": "Hiswe <hiswehalya@gmail.com>",
  "license": "MIT",
  "ava": {
    "files": [
      "shared/**/*.spec.js"
    ],
    "babel": false,
    "compileEnhancements": false,
    "require": [
      "@babel/register",
      "./test-browser-env.js"
    ]
  },
